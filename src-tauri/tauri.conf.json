{
  "productName": "leptos_tauri_from_scratch_tauri",
  "version": "0.1.0",
  "identifier": "com.tauri.dev0",
  "build": {
    "beforeDevCommand": "trunk serve --no-default-features -v --features \"csr\"",
    "beforeBuildCommand": "trunk build --no-default-features -v --features \"csr\"",
    "devUrl": "http://127.0.0.1:8080",
    "frontendDist": "../frontend/dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "template",
        "width": 800,
        "height": 600
      }
    ],
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
